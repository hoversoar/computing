FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

momentumSource
{
    type      meanVelocityForce;

    timeStart       0.0;
    duration        1000;
    selectionMode   all;
    
    fields          (U.air);
    Ubar            (0 0 0.24);
    relaxation      1.0;
}


